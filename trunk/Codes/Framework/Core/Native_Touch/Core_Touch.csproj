      <InProject>false</InProject>
    </MMP_STUB_Load>
    <MMP_STUB_Load Include="$(BUILD_TREE_PE)\Microsoft.SPOT.Native.pe">
      <InProject>false</InProject>
    </MMP_STUB_Load>
    <MMP_STUB_Load Include="$(BUILD_TREE_PE)\Microsoft.SPOT.Graphics.pe">
